<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/CurrentCapacity"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:layout_width="wrap_content">

    <TextView
        android:id="@+id/BuildingCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Building Code"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="146dp" />

    <TextView
        android:id="@+id/MaxCapacityLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maximum Capacity"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="245dp" />

    <TextView
        android:id="@+id/CurrentCapacity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="capacity"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="202dp" />

    <Button
        android:id="@+id/UpdateCapacityUI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onUpdateWithUI"
        android:text="Update Capacity"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="390dp" />

    <Button
        android:id="@+id/UpdateCapacityCSV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onUpdateWithCSV"
        android:text="Upload a File (CSV)"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="466dp" />

    <TextView
        android:id="@+id/MaxCapacity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="max "
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="247dp" />

    <TextView
        android:id="@+id/CurrentCapacityLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Capacity"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="202dp" />

</androidx.constraintlayout.widget.ConstraintLayout>